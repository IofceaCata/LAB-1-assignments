#include<iostream>
#include<string.h>
using namespace std;
int main(){
    int r,i;
    char a[100];
    cin.get(a,100);
    r=strlen(a);
    for(i=0;i<r;i++){
        if((a[i]>='a')&&(a[i]<='z'))
            a[i]=a[i]-32;
        else
        if((a[i]>='A')&&(a[i]<='Z'))
            a[i]=a[i]+32;
    }
    cout<<a;


}
